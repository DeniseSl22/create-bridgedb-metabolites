PREFIX wdt: <http://www.wikidata.org/prop/direct/>
SELECT (str(?compoundRes) as ?compound) ?key (str(?nameLit) as ?name) WHERE {
  ?compoundRes wdt:P31 wd:Q11173 .
  OPTIONAL {
    ?compoundRes rdfs:label ?nameLit
    FILTER((LANG(?nameLit)) = "en")
  }
  OPTIONAL {
    ?compoundRes wdt:P235 ?key
  }
}
